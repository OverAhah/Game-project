<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main9Activity">


    <TextView
        android:id="@+id/Empty1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text=""
        android:textSize="20sp"/>

    <TextView
        android:id="@+id/Empty2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Empty1"
        android:layout_centerHorizontal="true"
        android:text=""
        android:textSize="20sp"/>

    <TextView
        android:id="@+id/Text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Empty2"
        android:layout_centerHorizontal="true"
        android:text=""
        android:textColor="@android:color/black"
        android:textSize="20sp"/>

    <Button
        android:id="@+id/Answer91"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/Text2"
        android:text=""
        android:textSize="20sp" />

    <Button
        android:id="@+id/Answer92"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/Answer91"
        android:text=""
        android:textSize="20sp" />

    <Button
        android:id="@+id/Answer93"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/Answer92"
        android:text=""
        android:textSize="20sp"/>

    <Button
        android:id="@+id/Answer94"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Answer93"
        android:layout_centerHorizontal="true"
        android:text=""
        android:textSize="20sp"/>

    <Button
        android:id="@+id/Answer95"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Answer94"
        android:layout_centerHorizontal="true"
        android:text=""
        android:textSize="20sp"/>


</RelativeLayout>